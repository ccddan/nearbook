{
  "name": "web-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:contract && npm run build:web",
    "build:contract": "cd .. && node contract/compile.js",
    "build:contract:debug": "cd .. && node contract/compile.js --debug",
    "build:web": "react-scripts build",
    "dev:deploy:contract": "cd .. && near dev-deploy",
    "deploy:contract": "cd .. && near deploy",
    "deploy": "npm run build && npm run deploy:contract",
    "prestart": "npm run build:contract:debug && npm run dev:deploy:contract",
    "start": "echo The app is starting! It will automatically open in your browser when ready && env-cmd -f ../neardev/dev-account.env react-scripts start",
    "dev": "nodemon --watch ../contract -e ts --exec \"npm run start\"",
    "test": "npm run build:contract:debug && cd contract && npm run test && cd .. && react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "env-cmd": "~10.1.0",
    "jest": "26.6.0",
    "jest-environment-node": "26.6.0",
    "near-cli": "~2.1.1",
    "nodemon": "~2.0.3",
    "shelljs": "~0.8.4"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "near-api-js": "~0.43.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
